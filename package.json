{
  "name": "airport-route-api",
  "version": "1.0.0",
  "description": "Airport route API",
  "main": "index.js",
  "scripts": {
    "local-run-config" : "export NEO4J_HOST=localhost",
    "local-start" : "npm run local-run-config && npm run start",
    "start": "npm run build && npm run serve",
    "build": "npm run build-ts",
    "serve": "node dist/index.js",
    "test": "jest --forceExit --coverage --verbose",
    "watch-test": "npm run test -- --watchAll",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "search": "npm run local-run-config && npm run build-ts && node dist/scripts/search.js",
    "calc-distances": "npm run local-run-config && npm run build-ts && node  dist/scripts/paths.js"
  },
  "keywords": [
    "airports",
    "routing",
    "api"
  ],
  "author": "Margus Haavala",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.11.1",
    "@types/neo4j": "^2.0.2",
    "@types/node": "^7.0.65",
    "jest": "^22.0.4"
  },
  "dependencies": {
    "express": "^4.16.3",
    "haversine-distance": "^1.1.4",
    "neo4j-driver": "^1.5.0",
    "pm2": "^2.10.4",
    "ts-node": "^6.0.5",
    "typescript": "^2.8.3"
  }
}
